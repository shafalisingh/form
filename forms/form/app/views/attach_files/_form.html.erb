<%= simple_form_for(@attach_file) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :attachment_type %>
    <%= f.input :attachment_name %>
    <%= f.input :priority %>
    <%= f.input :maps_to %>
    <%= f.association :scrapy_config %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
